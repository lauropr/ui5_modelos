            "env": {
                "run.config": "{\"handlerId\":\"fiori_tools\",\"runnableId\":\"c:\\\\Users\\\\lauro\\\\projects\\\\modelos\"}",
                "FIORI_TOOLS_BACKEND_CONFIG": "[{\"path\":\"/sap\",\"url\":\"http://localhost\"}]",
                "FIORI_TOOLS_URL_PARAMS": "sap-ui-xx-viewCache=false&sap-language=EN"
            }
        },
